                   <span style="margin-left:10px"><i class="far fa-user"></i> <data:post.author/></span>
                 </div>

                 <div class='post-snippet'>
                   <b:if cond='data:blog.pageType == "item"'>
                     <data:post.body/>
                     
                     <!-- SHARE BUTTONS -->
                     <div style="text-align:center; margin-top:30px; padding:20px; background:#f9f9f9; border-radius:8px;">
                        <h4 style="text-transform:uppercase; color:#333; margin-bottom:15px;">Share This Story</h4>
                        <div class="share-btn-container">
                          <a class="share-btn" style="background:#1877F2" expr:href='&quot;https://www.facebook.com/sharer/sharer.php?u=&quot; + data:post.url' target="_blank"><i class="fa-brands fa-facebook-f"></i></a>
                          <a class="share-btn" style="background:black" expr:href='&quot;https://twitter.com/intent/tweet?url=&quot; + data:post.url + &quot;&amp;text=&quot; + data:post.title' target="_blank"><i class="fa-brands fa-x-twitter"></i></a>
                          <a class="share-btn" style="background:#BD081C" expr:href='&quot;https://pinterest.com/pin/create/button/?url=&quot; + data:post.url + &quot;&amp;media=&quot; + data:post.firstImageUrl' target="_blank"><i class="fa-brands fa-pinterest-p"></i></a>
                          <a class="share-btn" style="background:#25D366" expr:href='&quot;https://api.whatsapp.com/send?text=&quot; + data:post.title + &quot; &quot; + data:post.url' target="_blank"><i class="fa-brands fa-whatsapp"></i></a>
                          <a class="share-btn" style="background:#FF4500" expr:href='&quot;https://www.reddit.com/submit?url=&quot; + data:post.url + &quot;&amp;title=&quot; + data:post.title' target="_blank"><i class="fa-brands fa-reddit"></i></a>
                          <a class="share-btn" style="background:#0088cc" expr:href='&quot;https://t.me/share/url?url=&quot; + data:post.url + &quot;&amp;text=&quot; + data:post.title' target="_blank"><i class="fa-brands fa-telegram"></i></a>
                        </div>
                     </div>

                   <b:else/>
                     <data:post.snippet/>
                     <br/>
                     <a expr:href='data:post.url' class='read-story-btn'>READ STORY</a>
                   </b:if>
                 </div>
               </div>
             </b:includable>

             <b:includable id='nextprev'>
               <div class='blog-pager'>
                 <b:if cond='data:newerPageUrl'>
                   <a expr:href='data:newerPageUrl'>&#8592; Newer Posts</a>
                 </b:if>
                 <b:if cond='data:olderPageUrl'>
                   <a expr:href='data:olderPageUrl' style='float:right'>Older Posts &#8594;</a>
                 </b:if>
               </div>
             </b:includable>
          </b:widget>
        </b:section>
      </main>

    </div>

